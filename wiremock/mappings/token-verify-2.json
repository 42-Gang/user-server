{
  "request": {
    "method": "POST",
    "url": "/api/v1/auth/validate-token",
    "headers": 
      {
        "x-internal": {
          "equalTo": "true"
        },
        "authorization": {
          "equalTo": "Bearer 2"
        }
      }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json",
      "X-Authenticated": "true",
      "X-User-Id": "22"
    }
  }
}
